<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="img\complexNum.png" />
    <None Remove="img\complexNumPolar.png" />
    <None Remove="img\fragezeichen.jpg" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ComplexLib\ComplexLib.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="img\complexNum.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="img\complexNumPolar.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="img\fragezeichen.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
